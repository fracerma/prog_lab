<div class="nav">
    <% if !@current_user %>
        <div> <%= link_to  "Login", login_path, class: "btn btn-dark" %> </div>
        <%= link_to root_path do %>
            <%= image_tag("LogoWhereNext.png", :alt => "Where Next") %>
        <% end %>
        <div> <%= link_to  "Signup", signup_path, class: "btn btn-dark" %> </div>
    <% else%> 
        <div> <%= link_to  "Profile", user_path, class: "btn btn-dark" %> </div>
        <%= link_to root_path do %>
            <%= image_tag("LogoWhereNext.png", :alt => "Where Next") %>
        <% end %>
        <div> <%= link_to  "Logout", logout_path, class: "btn btn-dark" %> </div>
    <% end%>
</div>